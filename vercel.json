{
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "NODE_VERSION": "20"
  },
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
